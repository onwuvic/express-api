{
	"root": true,
	"extends": [
		"airbnb-base",
    "plugin:import/errors",
    "plugin:import/warnings"
	],
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module"
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"mocha": true
	},
	"rules": {
    "no-console": 1,
    "comma-dangle": 0
	}
}
